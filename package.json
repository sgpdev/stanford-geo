{
  "name": "SGPWebApp",
  "version": "1.0.0",
  "description": "SGPWebApp",
  "author": "Sufian Lattouf",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "starts": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start": "node --max-old-space-size=6144 server/index.js",
    "react-dev": "webpack -D -d --watch",
    "test": "jest",
    "mv_setup": "node ./models/script.js"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testMatch": [
      "**/client/src/components/**/*.test.{js,jsx}",
      "**/__tests__/*.{js}"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "testURL": "http://localhost",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
