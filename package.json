    "dotenv": "^6.1.0",
    "express": "^4.17.3",
    "express-basic-auth": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "npm": "^6.2.0",
    "pg": "^7.6.1",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-table-next": "^1.4.1",
