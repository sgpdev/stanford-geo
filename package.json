    "react-csv": "^1.0.19",
    "react-dom": "^16.4.1",
    "react-drawer": "^1.3.4",
    "react-fontawesome": "^1.6.1",
    "react-icons": "^3.2.2",
    "react-multi-toggle": "^1.1.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "react-sidebar": "^3.0.2",
    "react-table": "^6.8.6",
    "react-tooltip": "^3.9.0",
